<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                   http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.0.xsd">

    <changeSet id="2018_11_06_update_table_autoincrement_id" author="kashenkov">
        <addAutoIncrement tableName="administrator" columnName="administrator_id"/>
        <addAutoIncrement tableName="category_service" columnName="category_id"/>
        <addAutoIncrement tableName="chief" columnName="chief_id"/>
        <addAutoIncrement tableName="operator" columnName="operator_id"/>
        <addAutoIncrement tableName="queue" columnName="client_id"/>
        <addAutoIncrement tableName="service" columnName="service_id"/>
        <addAutoIncrement tableName="window" columnName="number_window"/>
    </changeSet>
</databaseChangeLog>